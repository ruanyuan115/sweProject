<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.sweproject.dao.UserDao">
    <insert id="insert" parameterType="com.example.sweproject.bean.User" useGeneratedKeys="true" keyProperty="userID">
        INSERT INTO USER SET userName=#{userName},password=#{password}
    </insert>
</mapper>